
{
  "manifest_version": 3,
  "name": "automobile.at Neueste adds ",
  "version": "1.0.0",
  "description": "SYMAT Script.",
  "icons": {
    "16": "newIcon.png",
    "32": "newIcon.png",
    "48": "newIcon.png",
    "128": "newIcon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "sidePanel", "storage", "contextMenus", "activeTab", "tabs"]  ,
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Tab HTML Saver",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "newIcon.png",
      "32": "newIcon.png",
      "48": "newIcon.png",
      "128": "newIcon.png"
    }

  },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
     
    }
  ],
 "web_accessible_resources": [{
    "resources": ["script.js"],
    "matches": ["<all_urls>"]
  }]
}

